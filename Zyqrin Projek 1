local WindUI = loadstring(game:HttpGet(
    "https://github.com/Footagesus/WindUI/releases/latest/download/main.lua"
))()

local Window = WindUI:CreateWindow({
    Title = "My Super Hub",
    Icon = "door-open",
    Author = "by .ftgs and .ftgs",
    Folder = "MySuperHub",

    Size = UDim2.fromOffset(580, 460),
    Transparent = true,
    Theme = "Dark",

    User = {
        Enabled = true,
        Anonymous = true,
    }
})

local InfoTab = Window:Tab({
    Title = "Tab Title",
    Icon = "bird", -- optional
    Locked = false,
})
local discordInvite = "https://discord.gg/INVITE_KAMU"

local Paragraph = InfoTab:Paragraph({
    Title = "Paragraph with Image, Thumbnail, Buttons",
    Desc = "Script ini memiliki fitur:
• Auto Farm
• Auto Collect
• Auto Buy
• Auto Rejoin

Catatan:
- Jangan pakai di akun utama
- Jika error, rejoin server
- Update otomatis tanpa ganti link

Discord:
discord.gg/INVITEKAMU",
    Color = "Red",
    Buttons = {
        {
            Icon = "bird",
            Title = "Join Discord",
            Callback = function()
                if setclipboard then
                    setclipboard(discordInvite)
                end

                game:GetService("StarterGui"):SetCore("SendNotification", {
                    Title = "Discord",
                    Text = "Link Discord disalin ke clipboard!",
                    Duration = 5
                })
            end,
        }
    }
})
